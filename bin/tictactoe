#!/usr/bin/env ruby

require_relative '../lib/tic_tac_toe'

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"
display_board(board)

def play(board)
  until over?
    turn(board)
  end
  if won?
     winner(board) = "X" || winner(board) = "O"
    puts "Congratulations #{}!"
  
  
  turn(board)
  end
end